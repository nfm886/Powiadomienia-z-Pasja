{
  "manifest_version": 2,
  "name": "Powiadomienia z Pasją",
  "description": "Szybki dostęp do powiadomień i obserwowanych tagów z Forum Pasja Informatyki",
  "version": "1.0",
  "icons": {
    "16": "media/images/icon.png",
    "48": "media/images/icon-48x48.png",
    "128": "media/images/icon-128x128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "media/images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://*.pasja-informatyki.pl/",
    "storage"
  ]
}